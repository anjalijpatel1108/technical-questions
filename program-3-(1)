# Problem 3:  Biggest and smallest number in an array
def find_max_min(arr): 
	max = arr[0] 
	min = arr[0] 
	for i in range(1, len(arr)): 
		if arr[i] > max: 
			max = arr[i] 
		if arr[i] < min: 
			min = arr[i] 
	return max, min

arr = [1, 2, 3, 4, 5] 
max, min = find_max_min(arr) 
print("Maximum is:", max) 
print("Minimum is:", min)
